{
    "name": "codeilia/larastic",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "http://codeilia.com/larastic",
    "license": "MIT",
    "authors": [
        {
            "name": "Milad rostami",
            "email": "milad.rostami.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "elasticsearch/elasticsearch": "6.*",
        "laravel/scout": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "mockery/mockery": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Larastic\\Tests\\": "tests/",
            "Larastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larastic\\LarasticServiceProvider"
            ]
        }
    }
}
